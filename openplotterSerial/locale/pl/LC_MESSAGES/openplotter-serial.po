msgstr "Serwer Signal K został zrestartowany"

#: openplotterSerial.py:740
msgid "Please install \"gpsd\" package"
msgstr "Proszę zainstalować pakiet \"gpsd\""

#: openplotterSerial.py:761
msgid "Pypilot serial devices modified and autopilot enabled"
msgstr "Urządzenie szeregowe Pypilot zmodyfikowane i autopilot włączony"

#: openplotterSerial.py:765
msgid "Please install \"Pypilot\" OpenPlotter app"
msgstr "Proszę zainstalować aplikację OpenPlotter \"Pypilot\""

#: openplotterSerial.py:797
msgid "Failed. Error removing connection in Signal K"
msgstr "Niepowodzenie. Błąd podczas usuwania połączenia w Signal K"

#: openplotterSerial.py:840
msgid "Adding connection for device: "
msgstr "Dodawanie połączenia dla urządzenia: "

#: openplotterSerial.py:847
msgid "Baud Rate: "
msgstr "Prędkość w bodach: "

#: openplotterSerial.py:854
msgid "MANUAL"
msgstr "RĘCZNIE"

#: openplotterSerial.py:856
msgid "AUTO"
msgstr "AUTOMATYCZNIE"

